public class recursion15{
    public static void main(String[] args){
        
    }
    
    public static void triangle(int r, int c){
        if(c < r){
            System.out.print("*");
        }
        if(c == r){
            System.out.println();
        }
        if(r == 1){
            return;
        }
        
    }
}